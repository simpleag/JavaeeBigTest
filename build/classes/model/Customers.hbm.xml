<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.Customers" table="customers">
		<id name="CustomerID" column="CustomerID">
			<generator class="assigned"/>
		</id>
		
		<property name="CompanyName" type="string" column="CompanyName"/>
		<property name="ContactName" type="string" column="ContactName"/>
		<property name="ContactTitle" type="string" column="ContactTitle"/>
		<property name="Address" type="string" column="Address"/>
		<property name="City" type="string" column="City"/>
		<property name="Region" type="string" column="Region"/>
		<property name="PostalCode" type="string" column="PostalCode"/>
		<property name="Country" type="string" column="Country"/>
		<property name="Phone" type="string" column="Phone"/>
		<property name="Fax" type="string" column="Fax"/>
	</class>
</hibernate-mapping>        